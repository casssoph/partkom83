	ТаблицаДвижений.Колонки.Добавить("ВидСписания", Новый ОписаниеТипов("Строка"));
	
	ТаблицаДвиженийVMI = Неопределено;
	ОбщегоНазначения.СоздатьСтруктуруРегистраНакопления("ПартииТоваровVMI", ТаблицаДвиженийVMI);
	
