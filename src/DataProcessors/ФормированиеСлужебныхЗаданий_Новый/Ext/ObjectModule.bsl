//Мегалогист++
Функция ДокументМаршрутноеЗадание(Реализация,ДокСЗ)
	
	Запрос = Новый Запрос("ВЫБРАТЬ
	                      |	МегаЛогист_МаршрутноеЗадание.Ссылка
	                      |ИЗ
	                      |	Документ.МегаЛогист_МаршрутноеЗадание КАК МегаЛогист_МаршрутноеЗадание
	                      |ГДЕ
	                      |	МегаЛогист_МаршрутноеЗадание.ТипДоставки = &ТипДоставки
	                      |	И МегаЛогист_МаршрутноеЗадание.Контрагент = &Контрагент
	                      |	И МегаЛогист_МаршрутноеЗадание.ДатаДоставки = &ДатаОтгрузки
	                      |	И МегаЛогист_МаршрутноеЗадание.ВремяДоставкиС = &ВремяОтгрузки");
	
	Запрос.УстановитьПараметр("ТипДоставки", Реализация.ТипДоставки);
	Запрос.УстановитьПараметр("Контрагент", Реализация.Контрагент);
	Запрос.УстановитьПараметр("ДатаОтгрузки", ДокСЗ.ДатаОтгрузки);
	Если Реализация.ТипДоставки=Справочники.ТипыДоставки.ЭкспрессДоставка тогда
		ВремяДоставкиС=ПроверкаРазницыВремени.ПолучитьРегиональноеВремя(Реализация.ВремяДоставкиС,Реализация.Склад);		
		Запрос.УстановитьПараметр("ВремяОтгрузки", ВремяДоставкиС);
	иначе	
		Запрос.УстановитьПараметр("ВремяОтгрузки", ДокСЗ.ВремяОтгрузки);
	КонецЕсли;
	
	Выборка = Запрос.Выполнить().Выбрать();
	
	Если Выборка.Следующий() Тогда
		Документ = Выборка.Ссылка.ПолучитьОбъект();
	Иначе
		Документ = Документы.МегаЛогист_МаршрутноеЗадание.СоздатьДокумент();
	КонецЕсли;
	
	Возврат Документ;
	
КонецФункции

Процедура СоздатьДокументыМЗ(ДокСЗ)
	лКлючАлгоритма = "Обработка_ФормированиеСлужебныхЗаданий_Новый_СоздатьДокументыМЗ"; // Вставить название процедуры
	лЗамена =  АлгоритмыПолучитьЗамену(лКлючАлгоритма);
	Если Не лЗамена = Неопределено Тогда
		Выполнить(лЗамена);
		Возврат;
	КонецЕсли;
	///////////////////////////////////////////////////////////////////////////
	
	//Временное ограничение только на Карпова М. Г. (рег.развитие)
	//N0021506
	ТекКонтрагент=Справочники.Контрагенты.НайтиПоКоду("N0021506");
	
	МассивМЗ = Новый Массив;
	
	Для Каждого СтрокаРеализации из ДокСЗ.Покупатели цикл
		
		Реализация=СтрокаРеализации.Реализация;
		Контрагент=Реализация.Контрагент;
		Филиал=Реализация.Филиал;
				
		//Если Контрагент<>ТекКонтрагент тогда
		//	Продолжить;
		//КонецЕсли;	
		Если Филиал<>Справочники.Филиалы.НижнийНовгород тогда
			Продолжить;
		КонецЕсли;
		
		МЗ = ДокументМаршрутноеЗадание(Реализация,ДокСЗ);
		//МЗ=Документы.МегаЛогист_МаршрутноеЗадание.СоздатьДокумент();
		
		МЗ.Дата=ТекущаяДата();
		ЗаполнитьЗначенияСвойств(МЗ, Реализация, "ДатаДоставки,ТипДоставки,Контрагент,express_delivery_request_id,Организация,Филиал,МаршрутДоставки");
		НоваяСтрока=МЗ.ДокументыРеализации.Добавить();
		НоваяСтрока.ДокументСсылка=Реализация.Ссылка;
	    МЗ.Статус = Перечисления.МегаЛогист_СтатусыМаршрутныхЗаданий.КРаспределению;
		МЗ.Ответственный  = ПараметрыСеанса.ТекущийПользователь;
		МЗ.ТипЗадания=Справочники.МегаЛогист_ТипыМаршрутныхЗаданий.ДоставкаДоКлиента;
		МЗ.АдресДоставки=Реализация.АдресДоставки;
		МЗ.ДатаДоставки=ДокСЗ.ДатаОтгрузки;
		МЗ.Телефон=Реализация.Телефон;
		МЗ.КонтактноеЛицоКонтрагента=Реализация.Заказчик;
		
		Если Реализация.ТипДоставки=Справочники.ТипыДоставки.ЭкспрессДоставка тогда
			МЗ.ЭкспрессДоставка=Истина;
			МЗ.ВремяДоставкиС=ПроверкаРазницыВремени.ПолучитьРегиональноеВремя(Реализация.ВремяДоставкиС,Реализация.Склад);
			МЗ.ВремяДоставкиПо=ПроверкаРазницыВремени.ПолучитьРегиональноеВремя(Реализация.ВремяДоставкиПо,Реализация.Склад);
		иначе
			МЗ.ЭкспрессДоставка=Ложь;
			//пока время такое, потом поменяем когда добавится в параметрах
			//приводить ли к региональному
			МЗ.ВремяДоставкиС=ДокСЗ.ВремяОтгрузки;
			МЗ.ВремяДоставкиПо=Реализация.ВремяДоставкиПо;	
		КонецЕсли;
	    
		МЗ.Записать(РежимЗаписиДокумента.Проведение);
		
