Процедура ДобавитьПричинуИсключения(ДокСЗ, ДокументРеализация, ДопроводимыеРТУ, ОписаниеОшибки, СтрокиТоваров)
	
	//Перем ЗаписьXML, НоваяСтрока, ПричинаИсключения, СсылкаНового, СтрокаТоваров;
	
	Если СтрНайти(ОписаниеОшибки, "lock") > 0 Тогда
		ПричинаИсключения = Перечисления.ПричиныНевыпискиРТУ.ТехническийСбойБлокировка;
	ИначеЕсли СтрНайти(ОписаниеОшибки, "КонтрольВозможностиВыписки") > 0 Тогда 
		ПричинаИсключения = Перечисления.ПричиныНевыпискиРТУ.КонтрольВыписки;
	ИначеЕсли СтрНайти(ОписаниеОшибки, "КонтрольОстатков") > 0 Тогда 
		ПричинаИсключения = Перечисления.ПричиныНевыпискиРТУ.НеХватаетТоваровНаСкладах;
	ИначеЕсли СтрНайти(ОписаниеОшибки, "ПогашениеПартийТоваров") > 0 Тогда 
		ПричинаИсключения = Перечисления.ПричиныНевыпискиРТУ.НеХватаетТоваровНаСкладах;
	ИначеЕсли СтрНайти(ОписаниеОшибки, "Служебный договор в заявке") > 0 Тогда 
		ПричинаИсключения = Перечисления.ПричиныНевыпискиРТУ.СлужебныйДоговорВЗаявке;	
	Иначе
		ПричинаИсключения = Перечисления.ПричиныНевыпискиРТУ.ТехническийСбойБлокировка;
	КонецЕсли;
	
	Если ПричинаИсключения = Перечисления.ПричиныНевыпискиРТУ.ТехническийСбойБлокировка Тогда 
		ПоместитьВДопроведение(ДокументРеализация, ДопроводимыеРТУ, СтрокиТоваров);
	Иначе
		Для Каждого СтрокаТоваров Из СтрокиТоваров Цикл 
			НоваяСтрока = ДокСЗ.НевыписанныеПокупатели.Добавить();
			ЗаполнитьЗначенияСвойств(НоваяСтрока, СтрокаТоваров);
			НоваяСтрока.ПричинаНевыписки =  ПричинаИсключения;
		КонецЦикла;
	КонецЕсли;

КонецПроцедуры

Процедура ПоместитьВДопроведение(ДокументРеализация, ДопроводимыеРТУ, СтрокиТоваров)
	
	//Перем ЗаписьXML, СсылкаНового, Структура;
	
	Если ДокументРеализация.ЭтоНовый() Тогда 
		СсылкаНового = ДокументРеализация.ПолучитьСсылкуНового();
		Если Не ЗначениеЗаполнено(СсылкаНового) Тогда 
			СсылкаНового = Документы.РеализацияТоваровУслуг.ПолучитьСсылку();
			ДокументРеализация.УстановитьСсылкуНового(СсылкаНового);
		КонецЕсли;
	Иначе
		СсылкаНового = ДокументРеализация.Ссылка;
	КонецЕсли;
	
	ЗаписьXML = Новый ЗаписьXML;
	ЗаписьXML.УстановитьСтроку();
	ЗаписатьXML(ЗаписьXML, ДокументРеализация);
	
	Структура = Новый Структура;
	Структура.Вставить("ЗаписьXML", ЗаписьXML.Закрыть());
	Структура.Вставить("СтрокиТоваров", СтрокиТоваров);
	
	ДопроводимыеРТУ.Вставить(СсылкаНового, Структура);

КонецПроцедуры

Процедура ДопровестиРТУ(ДопроводимыеРТУ, ДокСЗ)
	
	КоличествоПопыток = 5;
	
	Пока ДопроводимыеРТУ.Количество() > 0 И КоличествоПопыток > 0 Цикл 
		Для Каждого КЗ Из ДопроводимыеРТУ Цикл 
			НачатьТранзакцию(РежимУправленияБлокировкойДанных.Управляемый); //Для того чтобы рту попадало в сз сразу
			Попытка
				Структура = КЗ.Значение;
				ЧтениеXML = Новый ЧтениеXML;
				ЧтениеXML.УстановитьСтроку(Структура.ЗаписьXML);
				
				ДокументРеализация = ПрочитатьXML(ЧтениеXML);
				ДокументРеализация.Дата = ТекущаяДата();
				ДокументРеализация.ДополнительныеСвойства.Вставить("ОперативноеПроведение", Истина);
				СтрокиТоваров = Структура.СтрокиТоваров;
				
				ЗаписатьРТУ(ДокСЗ, ДокументРеализация);
				ЗафиксироватьТранзакцию();
				ДопроводимыеРТУ.Удалить(КЗ.Ключ);
			Исключение
				ОписаниеОшибки = ОписаниеОшибки();
				ОтменитьТранзакцию();
			КонецПопытки;
		КонецЦикла;
		КоличествоПопыток = КоличествоПопыток - 1;
	КонецЦикла;
	
	Для Каждого КЗ Из ДопроводимыеРТУ Цикл
		
		Структура = КЗ.Значение;
		ЧтениеXML = Новый ЧтениеXML;
		ЧтениеXML.УстановитьСтроку(Структура.ЗаписьXML);
		
		ДокументРеализация = ПрочитатьXML(ЧтениеXML);
		СтрокиТоваров = Структура.СтрокиТоваров;
		
		Для Каждого СтрокаТоваров Из СтрокиТоваров Цикл 
			НоваяСтрока = ДокСЗ.НевыписанныеПокупатели.Добавить();
			ЗаполнитьЗначенияСвойств(НоваяСтрока, СтрокаТоваров);
			НоваяСтрока.ПричинаНевыписки =  Перечисления.ПричиныНевыпискиРТУ.ТехническийСбойБлокировка;
		КонецЦикла;
		Сообщить("Не удалось создать РТУ на контрагента " + ДокументРеализация.ДоговорКонтрагента.Владелец);
	КонецЦикла;
КонецПроцедуры

Процедура ЗаписатьРТУ(ДокСЗ, ДокументРеализация, РегистрироватьВОбмене = Истина)
	
	лКлючАлгоритма = "Обработка_ФормированиеСлужебныхЗаданий_Новый_МодульОбъекта_ЗаписатьРТУ";
	лЗамена =  АлгоритмыПолучитьЗамену(лКлючАлгоритма);
	Если Не лЗамена = Неопределено Тогда
		Выполнить(лЗамена);
		Возврат;
	КонецЕсли;
		
	ДокументРеализация.Записать(РежимЗаписиДокумента.Проведение);
	
	СтрокаР = ДокСЗ.Покупатели.Найти(ДокументРеализация.Ссылка, "Реализация");
	Если СтрокаР = Неопределено Тогда 
		СтрокаР = ДокСЗ.Покупатели.Добавить();
	КонецЕсли;
	СтрокаР.Реализация = ДокументРеализация.Ссылка;
	СтрокаР.ТорговаяТочка = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(СтрокаР.Реализация, "ТорговаяТочка");
	СтрокаР.Регион = ОбщегоНазначения.ПолучитьЗначениеРеквизита(СтрокаР.ТорговаяТочка,"Регион");
	СтрокаР.ОрганизацияОплаты = ОбщегоНазначения.ПолучитьЗначениеРеквизита(СтрокаР.Реализация,"ДоговорКонтрагента.Организация");
	
	ДокСЗ.Записать();
	
	Если РегистрироватьВОбмене Тогда 
		//Регистрируем в обмен
		//Регистрируем в новый 11.10.2018 Валиахметов
		Узел = ОбменДаннымиКлиентСервер.ПолучитьИсходящийУзелОбмена(Метаданные.ПланыОбмена.ОбменПартКом83_TopLog_РТУ, 3);
		//Семенов И.П. 31.01.2019 XX-1768(
		//ПланыОбмена.ЗарегистрироватьИзменения(Узел, ДокументРеализация.Ссылка);
		ОбменДаннымиКлиентСервер.ЗарегистрироватьИзмененияВПланеОбмена(Узел, ДокументРеализация.Ссылка);
		//)Семенов И.П.
	КонецЕсли;
КонецПроцедуры


