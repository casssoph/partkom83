		<field xsi:type="DataSetFieldField">
			<dataPath>МенеджерЗаявки</dataPath>
			<field>МенеджерЗаявки</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Менеджер заявки</v8:content>
