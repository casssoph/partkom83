	КонецЕсли;
	Если ГлубинаНачало < ГлубинаКонец Тогда 
		Отказ = Истина;
		#Если Клиент Тогда 
			Предупреждение("Дата начала (глубина) должна быть больше даты конца", 2);
		#КонецЕсли
	КонецЕсли;
