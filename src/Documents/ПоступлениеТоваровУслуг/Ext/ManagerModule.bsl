		Строка = ТаблицаОшибок.Добавить();
		Строка.КлючСвязи = НомерСтроки;
	КонецЕсли;
	Строка.ОписаниеОшибки = Строка.ОписаниеОшибки + ?(Строка.ОписаниеОшибки = "", "", ";" + Символы.ПС) + СообщениеОбОшибке;
	Строка.ОписаниеОшибкиДляСайта = Строка.ОписаниеОшибкиДляСайта + ?(Строка.ОписаниеОшибкиДляСайта = "", "", ";" + Символы.ПС) + ОписаниеОшибкиДляСайта;
	Строка.ОтправлятьНаПочту = ОтправлятьНаПочту;
	Строка.Документ 		 = Документ;
	
