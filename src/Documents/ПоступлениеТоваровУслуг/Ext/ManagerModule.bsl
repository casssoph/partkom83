Функция НоменклатураЗамены(src_articul, src_brand, НомерСтроки, ТаблицаОшибок, Отказ)
	
	Номенклатура = Справочники.Номенклатура.ПустаяСсылка();
	Если ЗначениеЗаполнено(src_articul) Тогда
		Если ЗначениеЗаполнено(src_brand) Тогда
			Запрос = Новый Запрос("ВЫБРАТЬ
			                      |	Номенклатура.Ссылка
			                      |ИЗ
			                      |	Справочник.Номенклатура КАК Номенклатура
			                      |ГДЕ
			                      |	НЕ Номенклатура.ПометкаУдаления
			                      |	И Номенклатура.Артикул = &Артикул
			                      |	И Номенклатура.Изготовитель.Наименование = &Наименование");
			Запрос.УстановитьПараметр("Артикул", src_articul);
			Запрос.УстановитьПараметр("Наименование", src_brand);
			Выборка = Запрос.Выполнить().Выбрать();
			Если Выборка.Следующий() Тогда
				Номенклатура = Выборка.Ссылка;
			Иначе
				Отказ = Истина;
				СообщениеОбОшибке = "Не удалось найти номенклатуру замены (src_articul: " + src_articul + ", src_brand: " + src_brand + ")";
				СформироватьТекстОшибкиПоступления(ТаблицаОшибок, СообщениеОбОшибке, СообщениеОбОшибке, НомерСтроки);
			КонецЕсли;
		Иначе
			Отказ = Истина;
			СообщениеОбОшибке = "Указан артикул замены, но не указан бренд (src_articul: " + src_articul + ")";
			СформироватьТекстОшибкиПоступления(ТаблицаОшибок, СообщениеОбОшибке, СообщениеОбОшибке, НомерСтроки);
		КонецЕсли;
	КонецЕсли;
	
	Возврат Номенклатура;
	
КонецФункции
Функция НоменклатураПоступления(articul, src_articul, src_brand,  НомерСтроки, ТаблицаОшибок, Отказ)
	
	Номенклатура = Справочники.Номенклатура.ПустаяСсылка();
	Если ЗначениеЗаполнено(articul) И ЗначениеЗаполнено(src_articul) Тогда
		Если ЗначениеЗаполнено(src_brand) Тогда
			Запрос = Новый Запрос("ВЫБРАТЬ
			                      |	Номенклатура.Ссылка
			                      |ИЗ
			                      |	Справочник.Номенклатура КАК Номенклатура
			                      |ГДЕ
			                      |	НЕ Номенклатура.ПометкаУдаления
			                      |	И Номенклатура.Артикул = &Артикул
			                      |	И Номенклатура.Изготовитель.Наименование = &Наименование");
			Запрос.УстановитьПараметр("Артикул", articul);
			Запрос.УстановитьПараметр("Наименование", src_brand);
			Выборка = Запрос.Выполнить().Выбрать();
			Если Выборка.Следующий() Тогда
				Номенклатура = Выборка.Ссылка;
			Иначе
				Отказ = Истина;
				СообщениеОбОшибке = "Не удалось найти номенклатуру поступления (articul: " + articul + ", бренд товара заказа: " + src_brand + ")";
				СформироватьТекстОшибкиПоступления(ТаблицаОшибок, СообщениеОбОшибке, СообщениеОбОшибке, НомерСтроки);
			КонецЕсли;
		Иначе
			Отказ = Истина;
			СообщениеОбОшибке = "Указан артикул товара поступления, но не указан бренд товара заказа (articul: " + articul + ")";
			СформироватьТекстОшибкиПоступления(ТаблицаОшибок, СообщениеОбОшибке, СообщениеОбОшибке, НомерСтроки);
		КонецЕсли;
	КонецЕсли;
	
	Возврат Номенклатура;
	
КонецФункции
Функция НомерГТД(declaration_number)
	
	НомерГТД = Справочники.НомераГТД.ПустаяСсылка();
	Если НЕ НомерГДТПустой(ОчиститьНомерГТД(declaration_number)) Тогда
		НомерГТД = Справочники.НомераГТД.НайтиПоКоду(ОчиститьНомерГТД(declaration_number));
		Если НомерГТД.Пустая() Тогда
			НовыйНомерГТД = Справочники.НомераГТД.СоздатьЭлемент();
			НовыйНомерГТД.Код = ОчиститьНомерГТД(declaration_number);
			НовыйНомерГТД.Комментарий = "Загружено из окна поставщика: " + ТекущаяДата();
			Попытка 
				НовыйНомерГТД.Записать();
			Исключение
				ЗаписьЖурналаРегистрации("Создание ГТД",УровеньЖурналаРегистрации.Ошибка,,,"Ошибка записи ГТД с номером: "+declaration_number+" ### "+ОчиститьНомерГТД(declaration_number));
			КонецПопытки;				
			
			НомерГТД = НовыйНомерГТД.Ссылка;
		КонецЕсли;
	КонецЕсли;
	
	Возврат НомерГТД;
	
КонецФункции
Функция НомерГДТПустой(НомерГДТ)
	
	Запрос = Новый Запрос("ВЫБРАТЬ
	                      |	ИСТИНА КАК СодержитЦифры
	                      |ГДЕ
	                      |	&НомерГДТ ПОДОБНО ""%[0-9]%""");
	Запрос.УстановитьПараметр("НомерГДТ", НомерГДТ);
	Результат = Запрос.Выполнить();
	
	Возврат Результат.Пустой();
	
КонецФункции

Функция ОчиститьНомерГТД(Номер) Экспорт
	НовыйНомер = "";
	НужныеСимволы = "0123456789/Пп";
	Для Сч = 1 по СтрДлина(Номер) Цикл
		ТекущСимв = Сред(Номер, Сч, 1);
		Если Найти(НужныеСимволы, ТекущСимв) > 0 Тогда
			НовыйНомер = НовыйНомер + ТекущСимв;
		КонецЕсли;
	КонецЦикла;
	Возврат НовыйНомер;
КонецФункции

