Функция ОпределитьПриоритетОбработки(Массив) 
	
	лКлючАлгоритма = "Документ_РеализацияТоваровУслуг_МодульМенеджера_ОпределитьПриоритетОбработки";
	лЗамена =  АлгоритмыПолучитьЗамену(лКлючАлгоритма);
	Если Не лЗамена = Неопределено Тогда
		АлгоритмыЗначениеВозврата = Неопределено;		
		Выполнить(лЗамена);		
		Возврат АлгоритмыЗначениеВозврата;		
	КонецЕсли;	
	///////////////////////////////////////////////////////////////////////////

	Если ТипЗнч(Массив) <> Тип("Массив") Тогда
		МассивДокументов = Новый Массив;
		МассивДокументов.Добавить(Массив);
	КонецЕсли;
		
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	РеализацияТоваровУслуг.Контрагент КАК Контрагент,
	|	РеализацияТоваровУслуг.Контрагент.ЮрФизЛицо КАК ВидКлиента,
	|	РеализацияТоваровУслуг.ТипДоставки КАК ТипДоставки,
	|	РеализацияТоваровУслуг.МаршрутДоставки КАК МаршрутДоставки,
	|	РеализацияТоваровУслуг.Источник КАК Источник,
	|	РеализацияТоваровУслуг.Филиал КАК Филиал,
	|	РеализацияТоваровУслуг.Ссылка
	|ПОМЕСТИТЬ Документы
	|ИЗ
	|	Документ.РеализацияТоваровУслуг КАК РеализацияТоваровУслуг
	|ГДЕ
	|	РеализацияТоваровУслуг.Ссылка В(&МассивДокументов)
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	Документы.Ссылка,
	|	ПриоритетыОбработкиДокументовТопЛог.Контрагент,
	|	ПриоритетыОбработкиДокументовТопЛог.Филиал,
	|	ПриоритетыОбработкиДокументовТопЛог.МаршрутДоставки,
	|	ПриоритетыОбработкиДокументовТопЛог.ТипДоставки,
	|	ПриоритетыОбработкиДокументовТопЛог.Источник,
	|	ПриоритетыОбработкиДокументовТопЛог.ВидКлиента,
	|	ПриоритетыОбработкиДокументовТопЛог.Приоритет КАК Приоритет,
	|	ВЫБОР
	|		КОГДА ПриоритетыОбработкиДокументовТопЛог.Контрагент.Ссылка ЕСТЬ NULL
	|			ТОГДА 0
	|		ИНАЧЕ 1.3
	|	КОНЕЦ + ВЫБОР
	|		КОГДА ПриоритетыОбработкиДокументовТопЛог.Филиал.Ссылка ЕСТЬ NULL
	|			ТОГДА 0
	|		ИНАЧЕ 1.1
	|	КОНЕЦ + ВЫБОР
	|		КОГДА ПриоритетыОбработкиДокументовТопЛог.МаршрутДоставки.Ссылка ЕСТЬ NULL
	|			ТОГДА 0
	|		ИНАЧЕ 1.2
	|	КОНЕЦ + ВЫБОР
	|		КОГДА ПриоритетыОбработкиДокументовТопЛог.ТипДоставки.Ссылка ЕСТЬ NULL
	|			ТОГДА 0
	|		ИНАЧЕ 1
